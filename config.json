{
    "logging config": {
        "version": 1,
        "loggers": {
            "TimeTrack": {
                "level": "INFO",
                "handlers": [
                    "console_handler",
                    "file_handler"
                ]
            }
        },
        "handlers": {
            "console_handler": {
                "level": "WARNING",
                "formatter": "info",
                "class": "logging.StreamHandler"
            },
            "file_handler": {
                "level": "INFO",
                "formatter": "error",
                "class": "logging.FileHandler",
                "filename": "files/errorlogger.log"
            }
        },
        "formatters": {
            "info": {
                "format": "%(asctime)s %(levelname)4.4s >> line %(lineno)3s | %(module)s.%(funcName)s %(msg)s",
                "datefmt": "%Y-%m-%d %H:%M:%S"
            },
            "error": {
                "format": "%(asctime)s %(levelname)4.4s >> line %(lineno)3s | %(module)s.%(funcName)s %(msg)s",
                "datefmt": "%Y-%m-%d %H:%M:%S"
            }
        }
    },

